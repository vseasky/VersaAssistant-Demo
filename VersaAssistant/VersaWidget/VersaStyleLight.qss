/*
    %1  :   background-color:背景色
    %2  :   theme-color1: 标题栏颜色
    %3  :   theme-color2: 侧边栏颜色 
    %4  :   : 滑块等的颜色
    %5  :   hover: 鼠标停留颜色
    %6  :   chicked: 按下的颜色
    %7  :   selected: 选中的颜色
    %8  :   鼠标悬停字体颜色
*/

#logoButton {
    border: none;
    border-radius: 0px;
    background-color: transparent;
}

#logoButton:hover {
    background-color: transparent;
}

#logoButton:checked {
    background-color: transparent;
}


/*************************************************** QSplitter 样式设置 ***************************************************/

QSplitter {
    background-color: #aaaaaa;
    width: 2px;
    height: 2px;
}

QSplitter::handle:horizontal {
    background-color: #aaaaaa;
}

QSplitter::handle:vertical {
    background-color: #aaaaaa;
}

QSplitter::handle:hover {
    background-color: #aaaaaa;
}


/*************************************************** QTabWidget 样式设置 ***************************************************/
QTabWidget,
QTabWidget QWidget {
    background-color: %1;
    border: 1px solid rgba(0, 0, 0, 0.25);
}

QTabWidget::tab-bar {
    /* 设置对齐方式 */
    alignment: left;
}

QTabBar::tab {
    background-color: %1;
    color: black;
    padding: 8px;
    border: 1px solid rgba(0, 0, 0, 0.25);
    border-radius: 0px;
    border-bottom: none;
}

QTabBar::tab:hover {
    background-color: %5;
    color: %1;
}

QTabBar::tab:selected {
    background-color: %6;
    color: %1;
}

/*************************************************** QCheckBox 样式设置 ***************************************************/
QCheckBox {
    background-color: %1;
    padding-left: 8px;
    padding-right: 8px;
}

QCheckBox::indicator {
    background-color: %1;
    height: 24px;
    width: 24px;
    border-radius: 1px;
    background-image: url("://MingCute/system/check_fill.svg");
    border: 1px solid rgba(0, 0, 0, 0.25);
    background-repeat: no-repeat;
    background-position: center;
    color: black;
}


QCheckBox::indicator:hover {
    /* 主题颜色1 */
    background-color: %5;
    color: %1;
}

QCheckBox::indicator:checked {
    /* 选中颜色1 */
    background-color: %6;
    color: %1;
}

/*************************************************** QPushButton 样式设置 ***************************************************/
QPushButton {
    border: 1px solid rgba(0, 0, 0, 0.25);
    border-radius: 4px;
    background-color: %1;
    padding: 0px;
    color: black;
}

QTableWidget QPushButton {
    border: 1px solid rgba(0, 0, 0, 0.25);
    border-radius: 0px;
    margin: 4px;
    background-color: %1;
}

QPushButton:hover,
QTableWidget QPushButton:hover {
    /* 主题颜色1 */
    background-color: %5;
}

QPushButton:checked,
QTableWidget QPushButton:checked {
    /* 选中颜色1 */
    background-color: %6;
}

QMessageBox QPushButton
{
    height:32px;
    width:48px;
    border: 2px solid rgba(0, 0, 0, 0.25);
    border-radius: 4px;
    background-color: %1;
}

QMessageBox QPushButton:hover{
    /* 主题颜色1 */
    background-color: %5;
}

QMessageBox QPushButton:checked {
    /* 选中颜色1 */
    background-color: %6;
}


/******************************************* QTableWidget 样式设置 *******************************************/
QTableWidget {
    background-color: %1;
    padding: 0px;
}

QTableWidget::item {
    color: black;
    padding: 0px;
    background-color: %1;
}

QHeaderView::section,
QTableWidget::item,
QTableCornerButton:section {
    padding: 0px;
    margin: 0px;
    color: black;
    border: 1px solid rgba(0, 0, 0, 0.25);
    background-color: %1;
}

QHeaderView::section:hover,
QTableWidget::item:hover,
QTableCornerButton:section:hover {
    border: 1px solid %5;
}

QHeaderView::section:selected,
QTableWidget::item:selected,
QTableCornerButton:section:selected {
    border: 1px solid %7;
}


/******************************************* QMenuBar 样式设置 *******************************************/
QMenuBar,
QMenuBar::item {
    background-color: %1;
    color: %1;
}

QMenuBar::item::selected {
    /* 半透明遮罩 */
    background-color: rgba(128, 128, 128, 0.25);
}

QMenuBar QMenu {
    background-color: %1;
    color: black;
}

QMenuBar QMenu::item,
QMenuBar QMenu QWidgetAction,
QMenuBar QMenu QSpinBox,
QMenuBar QMenu QComboBox {
    background-color: %1;
    padding-left: 8px;
    padding-right: 16px;
    padding-top: 4px;
    padding-bottom: 4px;
    margin: 0px;
    min-width: 128px;
    height: 32px;
    border-radius: 2px;
    color: black;
}

QMenuBar QMenu QPushButton {
    background-color: %1;
    padding-left: 8px;
    padding-right: 16px;
    padding-top: 4px;
    padding-bottom: 4px;
    margin: 2px;
    height: 32px;
    width: 32px;
    max-width: 32px;
    border-radius: 2px;
}

QMenu::item:hover,
QMenu::item:selected,
QMenuBar QMenu::item:hover,
QMenuBar QMenu::item:selected,
QAction:hover,
QAction:selected,
QMenu QAction:hover,
QMenu QAction:selected,
QMenuBar QMenu QAction:hover,
QMenuBar QMenu QAction:selected {
    /* 主题颜色1 */
    background-color: %5;
    color: %1;
}

/* 选中的颜色 */
QMenu::item:checked,
QMenuBar QMenu::item:checked,
QAction:checked,
QMenu QAction:checked,
QMenuBar QMenu QAction:checked {
    /* 主题颜色1 */
    background-color: %7;
    color: %1;
}


/* non-exclusive indicator = check box style indicator (see QActionGroup::setExclusive) */
/* 未加入互斥的菜单指示器的未check状态，下同 */
/* QMenu::indicator:non-exclusive:unchecked {
    image: url(:/images/checkbox_unchecked.png);
} */

QMenu::indicator:non-exclusive:unchecked:selected {
    background-color: transparent;
    image: url(://MingCute/system/check_fill.svg);
}

QMenu::indicator:non-exclusive:checked {
    background-color: transparent;
    image: url(://MingCute/system/check_fill.svg);
}

/* QMenu::indicator:non-exclusive:checked:selected {
    image: url(:/images/checkbox_checked_hover.png);
} */


/*互斥的指示器状态，下同*/
/* QMenu::indicator:exclusive:unchecked {
    image: url(:/images/radiobutton_unchecked.png);
} */

QMenu::indicator:exclusive:unchecked:selected {
    background-color: transparent;
    image: url(://MingCute/system/check_fill.svg);
}

QMenu::indicator:exclusive:checked {
    background-color: transparent;
    image: url(://MingCute/system/check_fill.svg);
}

/* QMenu::indicator:exclusive:checked:selected {
    image: url(:/images/radiobutton_checked_hover.png);
} */


/*************************************************** QComboBox ***************************************************/
QComboBox {
    background-color: %1;
    border: 1px solid rgba(0, 0, 0, 48);
    border-radius: 0px;
}

/*下拉按钮QComboBox::drop-down*/
QComboBox::drop-down {
    min-width: 24px;
    subcontrol-origin: padding;
    subcontrol-position: top right;
}

/*箭头图标 QComboBox::down-arrow*/
QComboBox::down-arrow {
    min-width: 24px;
}

/*下拉列表QComboBox QAbstractItemView*/
QComboBox QAbstractItemView {
    border: 1px solid rgba(0, 0, 0, 48);
    outline: 0px;
    min-width: 160px;
}

QComboBox QAbstractItemView::item {
    background-color: %1;
    padding-left: 8px;
    padding-right: 16px;
    padding-top: 4px;
    padding-bottom: 4px;
    margin: 0px;
    height: 32px;
    border-radius: 0px;
    color: black;
    background-color: #FFFFFF;
}


/* 下拉菜单 */
QComboBox QAbstractItemView::item:hover,
QComboBox QAbstractItemView::item:selected {
    /* 主题颜色1 */
    background-color: %5;
    color: %1;
}


/* 需要注意一下 */
VersaLicenseWidget QPushButton {
    border: 1px solid white;
    border-radius: 4px;
    background-color: green;
    color: black;
}

VersaLicenseWidget QPushButton:hover {
